          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="zstd compressor - MAGIC" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import com.github.luben.zstd.*;
import java.util.Base64;

String body = vars.get(&quot;to_be_compressed&quot;);

byte[] uncompressed = body.getBytes();
String utf8Uncompressed = new String(uncompressed,&quot;UTF-8&quot;);
byte[] source = utf8Uncompressed.getBytes();
byte[] compressed = Zstd.compress(source,3);
byte[] encodedBytes = Base64.getEncoder().encode(compressed);

String payload_compressed = new String(encodedBytes, &quot;UTF-8&quot;);


vars.put(&quot;payload_compressed&quot;,payload_compressed);</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Sampler>
