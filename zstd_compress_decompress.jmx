          <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="zstd decompressor - MAGIC" enabled="true">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import com.github.luben.zstd.*;
import java.util.Base64;

String response = vars.get(&quot;payload_compressed&quot;);
byte[] decodedBytes = Base64.getDecoder().decode(response);
byte[] decompressed = Zstd.decompress(decodedBytes,4800000);
String responseMessage = new String(decompressed, &quot;UTF-8&quot;);

vars.put(&quot;responseText&quot;,responseMessage);
</stringProp>
